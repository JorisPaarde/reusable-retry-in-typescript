{
  "compilerOptions": {
    "target": "ES2020",
    "rootDir": "src",
    "types": [
      "node",
      "jest"
    ],
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "jsx": "react",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "module": "esnext",
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "sourceMap": true,
    "declarationMap": true
  }
}